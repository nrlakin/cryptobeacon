 123:hw_access.c   **** extern void InitHardware(void)
 124:hw_access.c   **** {
 125:hw_access.c   ****     /* Setup PIOs
 126:hw_access.c   ****      * PIO3 - Button
 127:hw_access.c   ****      * PIO9:11 - LEDs
 128:hw_access.c   ****      */
 129:hw_access.c   ****     /* First, set strong pulldowns as default. */
 130:hw_access.c   ****     PioSetModes(0xFFFFFFFFUL, pio_mode_user);
 131:hw_access.c   ****     PioSetDirs(0xFFFFFFFFUL, FALSE);
 132:hw_access.c   ****     PioSetPullModes(0xFFFFFFFFUL, pio_mode_strong_pull_down);
 133:hw_access.c   **** 
 134:hw_access.c   ****     /* Set the Button PIO direction as input */
 135:hw_access.c   ****     PioSetDir(BUTTON_PIO, PIO_DIRECTION_INPUT);
 136:hw_access.c   **** 
 137:hw_access.c   ****     /* Pull up the PIO */
 138:hw_access.c   ****     PioSetPullModes(BUTTON_PIO_MASK, pio_mode_strong_pull_up);
 139:hw_access.c   **** 
 140:hw_access.c   ****     /* Request an event when the button PIO changes state */
 141:hw_access.c   ****     PioSetEventMask(BUTTON_PIO_MASK, pio_event_mode_both);
 142:hw_access.c   **** 
 143:hw_access.c   ****     /* Set LEDs as outputs, default high (LED off) */
 144:hw_access.c   ****     PioSetDirs(0x00000E00UL, TRUE);     // LEDs are set to output high (off)
 145:hw_access.c   ****     PioSets(0x00000E00UL, 0x00000E00UL);
 146:hw_access.c   ****     PioSetPullModes(0x00000E00UL, pio_mode_no_pulls);
 147:hw_access.c   **** 
 148:hw_access.c   ****     /* Save power by changing the I2C pull mode to pull down.*/
 149:hw_access.c   ****     PioSetI2CPullMode(pio_i2c_pull_mode_strong_pull_down);
 150:hw_access.c   **** 
 151:hw_access.c   **** }
