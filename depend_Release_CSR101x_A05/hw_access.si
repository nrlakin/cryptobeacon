  77:hw_access.c   ****  *      handleExtraLongButtonPress
  78:hw_access.c   ****  *
  79:hw_access.c   ****  *  DESCRIPTION
  80:hw_access.c   ****  *      This function contains handling of extra long button press, which
  81:hw_access.c   ****  *      triggers pairing / bonding removal.
  82:hw_access.c   ****  *
  83:hw_access.c   ****  *  PARAMETERS
  84:hw_access.c   ****  *      tid [in]                ID of timer that has expired
  85:hw_access.c   ****  *
  86:hw_access.c   ****  *  RETURNS
  87:hw_access.c   ****  *      Nothing
  88:hw_access.c   ****  *----------------------------------------------------------------------------*/
  89:hw_access.c   **** static void handleExtraLongButtonPress(timer_id tid)
  90:hw_access.c   **** {
  91:hw_access.c   ****     if(tid == g_app_hw_data.button_press_tid)
  92:hw_access.c   ****     {
  93:hw_access.c   ****         /* Re-initialise button press timer */
  94:hw_access.c   ****         g_app_hw_data.button_press_tid = TIMER_INVALID;
  95:hw_access.c   **** 
  96:hw_access.c   ****         /* Sound three beeps to indicate pairing removal to user */
  97:hw_access.c   ****         // SoundBuzzer(buzzer_beep_thrice);
  98:hw_access.c   **** 
  99:hw_access.c   ****         /* Handle pairing removal */
 100:hw_access.c   ****         HandlePairingRemoval();
 101:hw_access.c   **** 
 102:hw_access.c   ****     } /* Else ignore timer */
 103:hw_access.c   **** 
 104:hw_access.c   **** }
